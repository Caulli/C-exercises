#include <iostream>

int TwoSum(int arr[], int sum){
    for(int i=0;i<6;i++){
        for(int j=0;j<6;j++){
            if(arr[i] + arr[j] == sum){
                std::cout << "Numbers " << arr[i] << " + " << arr[j] << " = " << sum << std::endl;
                return 0;
            }
        }
    }
}

int main() {
    int arr[] = {1,3,5,6,11,23};
    int sum = 9;
    TwoSum(arr,sum);
    return 0;
}
