#include <iostream>
#include <vector>

std::vector< std::vector<int> > squarePatch(int n) {
    std::vector<int> m(n,n);
    std::vector<std::vector<int> > v2d2(n,m);
    for(int i = 0; i < v2d2.size();i++){
        for(int j =0; j < v2d2[i].size();j++){
            std::cout << v2d2[i][j] << " ";
        }
        std::cout << std::endl;
    }
    return v2d2;
}


int main() {
    squarePatch(3);
    squarePatch(5);
    return 0;
}
